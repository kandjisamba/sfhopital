{% extends 'base.html.twig' %}

{% block title %}Inscrption{% endblock %}

{% block body %}
<div class="container">
 <br />
<h3 style="position:relative; text-align:center;">Ajouter un Medcin</h3>
 <br />
  <br />
  <div class="row">
      <div class="lg-col-10" style="position:relative; left:300px">
{{ form_start(form,{'action': path("medcin_new")})}}

<div class="form-group">
  {{form_row(form.prenom,{'attr': {'class': 'form-control'}})}}
</div>
<div class="form-group">
  {{form_row(form.nom,{'attr': {'class': 'form-control'}})}}
</div>
<div class="form-group">
  {{form_row(form.tel,{'attr': {'class': 'form-control'}})}}
</div>
</div>
 <div class="md-col-6" style="position:relative; left:400px">
<div class="form-group">
  {{form_row(form.datenais,{'attr': {'class': 'form-control form-control-md'}})}}
  
</div>
<div class="form-group">
  {{form_row(form.service,{'attr': {'class': 'form-control'}})}}
</div>
<label>Specialite</label>
<div class="form-group">

    {{ form_widget(form.specialites,{'attr': {'class': 'custum-select mr-md-2'}})}}
</div>
  </div>   
  </div>
  <br />
  <br />
  <br />  
<div class="form-group">
    <button type="submit" class="btn btn-primary" style="position:relative; left:500px">Enregistrer</button>
    </div>
{{ form_end(form)}}
    
</div>
{% endblock %}
